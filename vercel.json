{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "server/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "server/$1"
      }
    ]
  }
  